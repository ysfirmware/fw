 129:lib//src/stm32f10x_usart.c **** /*******************************************************************************
 130:lib//src/stm32f10x_usart.c **** * Function Name  : USART_Init
 131:lib//src/stm32f10x_usart.c **** * Description    : Initializes the USARTx peripheral according to the specified
 132:lib//src/stm32f10x_usart.c **** *                  parameters in the USART_InitStruct .
 133:lib//src/stm32f10x_usart.c **** * Input          : - USARTx: Select the USART or the UART peripheral. 
 134:lib//src/stm32f10x_usart.c **** *                    This parameter can be one of the following values:
 135:lib//src/stm32f10x_usart.c **** *                     - USART1, USART2, USART3, UART4 or UART5.
 136:lib//src/stm32f10x_usart.c **** *                  - USART_InitStruct: pointer to a USART_InitTypeDef structure
 137:lib//src/stm32f10x_usart.c **** *                    that contains the configuration information for the
 138:lib//src/stm32f10x_usart.c **** *                    specified USART peripheral.
 139:lib//src/stm32f10x_usart.c **** * Output         : None
 140:lib//src/stm32f10x_usart.c **** * Return         : None
 141:lib//src/stm32f10x_usart.c **** *******************************************************************************/
 142:lib//src/stm32f10x_usart.c **** void USART_Init(USART_TypeDef* USARTx, USART_InitTypeDef* USART_InitStruct)
 143:lib//src/stm32f10x_usart.c **** {
 144:lib//src/stm32f10x_usart.c ****   u32 tmpreg = 0x00, apbclock = 0x00;
 145:lib//src/stm32f10x_usart.c ****   u32 integerdivider = 0x00;
 146:lib//src/stm32f10x_usart.c ****   u32 fractionaldivider = 0x00;
 147:lib//src/stm32f10x_usart.c ****   u32 usartxbase = 0;
 148:lib//src/stm32f10x_usart.c ****   RCC_ClocksTypeDef RCC_ClocksStatus;
 149:lib//src/stm32f10x_usart.c **** 
 150:lib//src/stm32f10x_usart.c ****   /* Check the parameters */
 151:lib//src/stm32f10x_usart.c ****   assert_param(IS_USART_ALL_PERIPH(USARTx));
 152:lib//src/stm32f10x_usart.c ****   assert_param(IS_USART_BAUDRATE(USART_InitStruct->USART_BaudRate));  
 153:lib//src/stm32f10x_usart.c ****   assert_param(IS_USART_WORD_LENGTH(USART_InitStruct->USART_WordLength));
 154:lib//src/stm32f10x_usart.c ****   assert_param(IS_USART_STOPBITS(USART_InitStruct->USART_StopBits));
 155:lib//src/stm32f10x_usart.c ****   assert_param(IS_USART_PARITY(USART_InitStruct->USART_Parity));
 156:lib//src/stm32f10x_usart.c ****   assert_param(IS_USART_MODE(USART_InitStruct->USART_Mode));
 157:lib//src/stm32f10x_usart.c ****   assert_param(IS_USART_HARDWARE_FLOW_CONTROL(USART_InitStruct->USART_HardwareFlowControl));
 158:lib//src/stm32f10x_usart.c ****   /* The hardware flow control is available only for USART1, USART2 and USART3 */          
 159:lib//src/stm32f10x_usart.c ****   assert_param(IS_USART_PERIPH_HFC(USARTx, USART_InitStruct->USART_HardwareFlowControl));
 160:lib//src/stm32f10x_usart.c ****   
 161:lib//src/stm32f10x_usart.c ****   usartxbase = (*(u32*)&USARTx);
 162:lib//src/stm32f10x_usart.c **** 
 163:lib//src/stm32f10x_usart.c **** /*---------------------------- USART CR2 Configuration -----------------------*/
 164:lib//src/stm32f10x_usart.c ****   tmpreg = USARTx->CR2;
 165:lib//src/stm32f10x_usart.c ****   /* Clear STOP[13:12] bits */
 166:lib//src/stm32f10x_usart.c ****   tmpreg &= CR2_STOP_CLEAR_Mask;
 167:lib//src/stm32f10x_usart.c **** 
 168:lib//src/stm32f10x_usart.c ****   /* Configure the USART Stop Bits, Clock, CPOL, CPHA and LastBit ------------*/
 169:lib//src/stm32f10x_usart.c ****   /* Set STOP[13:12] bits according to USART_StopBits value */
 170:lib//src/stm32f10x_usart.c ****   tmpreg |= (u32)USART_InitStruct->USART_StopBits;
 171:lib//src/stm32f10x_usart.c ****   
 172:lib//src/stm32f10x_usart.c ****   /* Write to USART CR2 */
 173:lib//src/stm32f10x_usart.c ****   USARTx->CR2 = (u16)tmpreg;
 174:lib//src/stm32f10x_usart.c **** 
 175:lib//src/stm32f10x_usart.c **** /*---------------------------- USART CR1 Configuration -----------------------*/
 176:lib//src/stm32f10x_usart.c ****   tmpreg = USARTx->CR1;
 177:lib//src/stm32f10x_usart.c ****   /* Clear M, PCE, PS, TE and RE bits */
 178:lib//src/stm32f10x_usart.c ****   tmpreg &= CR1_CLEAR_Mask;
 179:lib//src/stm32f10x_usart.c **** 
 180:lib//src/stm32f10x_usart.c ****   /* Configure the USART Word Length, Parity and mode ----------------------- */
 181:lib//src/stm32f10x_usart.c ****   /* Set the M bits according to USART_WordLength value */
 182:lib//src/stm32f10x_usart.c ****   /* Set PCE and PS bits according to USART_Parity value */
 183:lib//src/stm32f10x_usart.c ****   /* Set TE and RE bits according to USART_Mode value */
 184:lib//src/stm32f10x_usart.c ****   tmpreg |= (u32)USART_InitStruct->USART_WordLength | USART_InitStruct->USART_Parity |
 185:lib//src/stm32f10x_usart.c ****             USART_InitStruct->USART_Mode;
 186:lib//src/stm32f10x_usart.c **** 
 187:lib//src/stm32f10x_usart.c ****   /* Write to USART CR1 */
 188:lib//src/stm32f10x_usart.c ****   USARTx->CR1 = (u16)tmpreg;
 189:lib//src/stm32f10x_usart.c **** 
 190:lib//src/stm32f10x_usart.c **** /*---------------------------- USART CR3 Configuration -----------------------*/  
 191:lib//src/stm32f10x_usart.c ****   tmpreg = USARTx->CR3;
 192:lib//src/stm32f10x_usart.c ****   /* Clear CTSE and RTSE bits */
 193:lib//src/stm32f10x_usart.c ****   tmpreg &= CR3_CLEAR_Mask;
 194:lib//src/stm32f10x_usart.c **** 
 195:lib//src/stm32f10x_usart.c ****   /* Configure the USART HFC -------------------------------------------------*/
 196:lib//src/stm32f10x_usart.c ****   /* Set CTSE and RTSE bits according to USART_HardwareFlowControl value */
 197:lib//src/stm32f10x_usart.c ****   tmpreg |= USART_InitStruct->USART_HardwareFlowControl;
 198:lib//src/stm32f10x_usart.c **** 
 199:lib//src/stm32f10x_usart.c ****   /* Write to USART CR3 */
 200:lib//src/stm32f10x_usart.c ****   USARTx->CR3 = (u16)tmpreg;
 201:lib//src/stm32f10x_usart.c **** 
 202:lib//src/stm32f10x_usart.c **** /*---------------------------- USART BRR Configuration -----------------------*/
 203:lib//src/stm32f10x_usart.c ****   /* Configure the USART Baud Rate -------------------------------------------*/
 204:lib//src/stm32f10x_usart.c ****   RCC_GetClocksFreq(&RCC_ClocksStatus);
 205:lib//src/stm32f10x_usart.c ****   if (usartxbase == USART1_BASE)
 206:lib//src/stm32f10x_usart.c ****   {
 207:lib//src/stm32f10x_usart.c ****     apbclock = RCC_ClocksStatus.PCLK2_Frequency;
 208:lib//src/stm32f10x_usart.c ****   }
 209:lib//src/stm32f10x_usart.c ****   else
 210:lib//src/stm32f10x_usart.c ****   {
 211:lib//src/stm32f10x_usart.c ****     apbclock = RCC_ClocksStatus.PCLK1_Frequency;
 212:lib//src/stm32f10x_usart.c ****   }
 213:lib//src/stm32f10x_usart.c **** 
 214:lib//src/stm32f10x_usart.c ****   /* Determine the integer part */
 215:lib//src/stm32f10x_usart.c ****   integerdivider = ((0x19 * apbclock) / (0x04 * (USART_InitStruct->USART_BaudRate)));
 216:lib//src/stm32f10x_usart.c ****   tmpreg = (integerdivider / 0x64) << 0x04;
 217:lib//src/stm32f10x_usart.c **** 
 218:lib//src/stm32f10x_usart.c ****   /* Determine the fractional part */
 219:lib//src/stm32f10x_usart.c ****   fractionaldivider = integerdivider - (0x64 * (tmpreg >> 0x04));
 220:lib//src/stm32f10x_usart.c ****   tmpreg |= ((((fractionaldivider * 0x10) + 0x32) / 0x64)) & ((u8)0x0F);
 221:lib//src/stm32f10x_usart.c **** 
 222:lib//src/stm32f10x_usart.c ****   /* Write to USART BRR */
 223:lib//src/stm32f10x_usart.c ****   USARTx->BRR = (u16)tmpreg;
 224:lib//src/stm32f10x_usart.c **** }
